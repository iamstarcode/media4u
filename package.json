{
  "name": "media4u",
  "author": "Iamstarcode.",
  "scripts": {
    "build": "pnpm -r build",
    "animepahe": "pnpm -F @iamstarcode/4u-lib build && pnpm -F anim4u animepahe",
    "animepahe2": "pnpm -F @iamstarcode/4u-lib build && pnpm -F anim4u animepahe2",
    "gogo": "pnpm -F @iamstarcode/4u-lib build && pnpm -F anim4u gogo",
    "aniwave": "pnpm -F @iamstarcode/4u-lib build && pnpm -F anim4u aniwave",
    "flix": "pnpm -F @iamstarcode/4u-lib build && pnpm -F flix4u flixhq",
    "mhd": "pnpm --filter @iamstarcode/4u-lib build && pnpm --filter flix4u mhd",
    "vidsrcto": "pnpm --filter @iamstarcode/4u-lib build && pnpm --filter flix4u vidsrcto",
    "hdrezka": "pnpm --filter @iamstarcode/4u-lib build && pnpm --filter flix4u hdrezka",
    "warezcdn": "pnpm --filter @iamstarcode/4u-lib build && pnpm --filter flix4u warezcdn",
    "prepare": "husky install"
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "husky": "^8.0.0"
  }
}
